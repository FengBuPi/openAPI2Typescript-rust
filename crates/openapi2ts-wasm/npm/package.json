{
	"name": "openapi2ts-wasm",
	"version": "0.1.0",
	"description": "openapi typescript",
	"keywords": [
		"openapi",
		"swagger",
		"typescript",
		"types",
		"codegen",
		"api",
		"wasm",
		"rust",
		"cli"
	],
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "tsc -p tsconfig.json && wasm-pack build --target nodejs --out-dir npm/pkg --release",
		"postbuild": "node -e \"const fs=require('fs'); const ps=['dist/index.js','dist/cli.js']; for (const p of ps) { if(!fs.existsSync(p)) continue; let s=fs.readFileSync(p,'utf8'); s=s.replaceAll('./pkg/', '../pkg/'); if(!s.startsWith('#!/usr/bin/env node')) s='#!/usr/bin/env node\\n'+s; fs.writeFileSync(p,s); }\"",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"prepack": "pnpm run build"
	},
	"bin": {
		"openapi2ts-wasm": "./dist/cli.js"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./cli": {
			"import": "./dist/cli.js"
		},
		"./wasm": {
			"types": "./pkg/openapi2ts_wasm.d.ts",
			"import": "./pkg/openapi2ts_wasm.js"
		}
	},
	"files": [
		"dist/**",
		"pkg/**"
	],
	"dependencies": {
		"swagger2openapi": "^7.0.8",
		"cosmiconfig": "^9.0.0"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"@types/swagger2openapi": "^7.0.4",
		"openapi-types": "^12.1.3",
		"typescript": "^5.3.0"
	},
	"publishConfig": {
		"access": "public"
	}
}